{
    "name": "@arosaned/project-generator",
    "version": "0.0.1",
    "description": "Project that contains cli tools and templates to generate npm based projects",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "jest",
        "start": "ts-node src/index.ts",
        "build": "tsc && shx rm -rf dist/templates && shx cp -r templates dist",
        "install-globally": "npm run build && npm uninstall -g && npm install -g"
    },
    "bin": {
        "arsd-generate-project": "./dist/index.js"
    },
    "author": "AroSaneD",
    "license": "MIT",
    "devDependencies": {
        "@types/ejs": "^3.1.0",
        "@types/inquirer": "^7.3.3",
        "@types/node": "^16.4.13",
        "@types/jest": "^27.0.2",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "eslint": "^8.3.0",
        "eslint-config-airbnb": "^19.0.1",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "jest": "^27.3.1",
        "nodemon": "^2.0.12",
        "prettier": "^2.5.0",
        "shx": "^0.3.3",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "chalk": "^5.0.0",
        "ejs": "^3.1.6",
        "inquirer": "^8.1.2",
        "inversify": "^6.0.1",
        "jsonc-parser": "^3.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.4.0"
    }
}
