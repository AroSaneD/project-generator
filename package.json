{
    "name": "@arosaned/project-generator",
    "version": "0.0.1",
    "description": "Project that contains cli tools and templates to generate npm based projects",
    "main": "dist/Startup.js",
    "types": "dist/Startup.d.ts",
    "scripts": {
        "test": "jest",
        "start": "ts-node ./Startup.ts",
        "build": "tsc && shx rm -rf dist/templates && shx cp -r templates dist",
        "install-globally": "npm run build && npm uninstall -g && npm install -g"
    },
    "bin": {
        "arsd-generate-project": "./dist/Startup.js"
    },
    "author": "AroSaneD",
    "license": "MIT",
    "devDependencies": {
        "@types/ejs": "^3.1.1",
        "@types/inquirer": "^7.3.3",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.3",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "eslint": "^8.27.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^16.2.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "jest": "^27.5.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "shx": "^0.3.4",
        "ts-jest": "^27.1.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "ejs": "^3.1.8",
        "inquirer": "^8.2.5",
        "inversify": "^6.0.1",
        "jsonc-parser": "^3.2.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.5.7"
    }
}
